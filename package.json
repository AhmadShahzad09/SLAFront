{
  "name": "sla-tool",
  "version": "1.0.0",
  "scripts": {
    "update-library": "node build-utils/update-index-file.js",
    "dev": "npm run update-library && vue-cli-service serve",
    "prod": "vue-cli-service build && cp src/assets/test-service.html ./dist/",
    "build:utils": "npx babel src/addons --out-dir lib --config-file ./babel.addons.config.js",
    "build": "node ./build-utils/build-lib.js && npm run build:utils",
    "deploy:beta": "npm run build && sh build-utils/deploy-beta.sh",
    "deploy": "npm run build && sh build-utils/deploy.sh",
    "docs": "./node_modules/.bin/jsdoc -r src -c jsdoc.json"
  },
  "main": "lib/index.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@vue/composition-api": "^1.6.2",
    "apisauce": "^2.0.0",
    "async-validator": "^3.2.3",
    "axios": "^0.25.0",
    "core-js": "^2.6.11",
    "d3": "^6.2.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.4.5",
    "echarts": "^4.9.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "node-sass": "^4.14.1",
    "normalize-wheel": "^1.0.1",
    "numeral": "^2.0.6",
    "resize-observer-polyfill": "^1.5.1",
    "sass-loader": "^8.0.2",
    "throttle-debounce": "^1.0.1",
    "vue": "^2.6.11",
    "vue-advanced-cropper": "^1.10.1",
    "vue-chartist": "^2.2.1",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-i18n": "^8.15.4",
    "vue-json-viewer": "^2.2.22",
    "vue-router": "^3.1.6",
    "vuex": "^3.5.1",
    "vuex-persist": "^3.1.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "better-docs": "^2.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jsdoc": "^3.6.4",
    "jsdoc-vuejs": "^3.0.8",
    "jsdoc-vuex-plugin": "^1.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-resources-loader": "^2.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
